<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xmy.demophoto.mapper.RowMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xmy.demophoto.entity.Row">
        <id column="Id" property="Id" />
        <result column="rowname" property="rowname" />
        <result column="content" property="content" />
        <result column="url" property="url" />
        <result column="address" property="address" />
        <result column="prCollectionNum" property="prCollectionNum" />
        <result column="place" property="place" />
        <result column="userid" property="userid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        Id, rowname, content, url, address, prCollectionNum, place, userid
    </sql>

</mapper>
